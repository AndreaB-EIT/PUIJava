<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FilePickerController">
   <children> 
      <Button fx:id="btnAccept" layoutX="14.0" layoutY="251.0" onAction="#onAccept" prefHeight="35.0" prefWidth="65.0" style="-fx-background-color: #8181F7;" text="Accept" textFill="#f2f2f2" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Button fx:id="btnFileDialog" layoutX="25.0" layoutY="75.0" onAction="#onFileDialog" style="-fx-background-color: #8181F7;" text="Open file explorer" textFill="#f2f2f2" AnchorPane.rightAnchor="188.0" AnchorPane.topAnchor="75.0" />
      <Button fx:id="btnCancel" layoutX="218.0" layoutY="351.0" onAction="#onCancel" prefHeight="35.0" prefWidth="65.0" style="-fx-background-color: #8181F7;" text="Cancel" textFill="#f2f2f2" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Label layoutX="25.0" layoutY="48.0" text="Selected file:" />
      <Label fx:id="path" layoutX="136.0" layoutY="48.0" /> 
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="32.0" left="32.0" right="32.0" top="32.0" />
   </padding>
</AnchorPane>
